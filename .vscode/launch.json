{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to debugpy (Uvicorn)",
      "type": "debugpy",
      "request": "attach",
      "connect": { "host": "localhost", "port": 5678 },
      "pathMappings": [
        { "localRoot": "${workspaceFolder}", "remoteRoot": "${workspaceFolder}" }
      ],
      "justMyCode": false
    }
  ]
}